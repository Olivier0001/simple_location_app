@page
@model SimpleLocationWeb.Pages.Admin.Users.IndexModel



 <div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">User List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-area="Identity" asp-page="/Account/Register" class="btn btn-primary">
                <i class="bi bi-pencil-square"></i> &nbsp;Create New User
            </a>
        </div>
    </div>
   
    <br /><br />
    <table id="DT_load" class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>UserName</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>FirstName</th>
                <th>LastName</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>

@section Scripts{
    <script src="~/js/user.js"></script>
}

